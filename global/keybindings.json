[
    //! Built-in


    //* Save w/ CTRL+S. Format save w/ CTRL+SHIFT+S. Save-as w/ CTRL+ALT+S
    //? The "editor.formatOnSave" key must be set in "settings.json"
    {
        "key": "ctrl+s",
        "command": "workbench.action.files.saveWithoutFormatting"
    },
    {
        "key": "ctrl+k ctrl+shift+s",
        "command": "-workbench.action.files.saveWithoutFormatting"
    },
    {
        "key": "ctrl+shift+s",
        "command": "workbench.action.files.save"
    },
    {
        "key": "ctrl+s",
        "command": "-workbench.action.files.save"
    },
    {
        "key": "ctrl+alt+s",
        "command": "workbench.action.files.saveAs"
    },
    {
        "key": "ctrl+shift+s",
        "command": "-workbench.action.files.saveAs"
    },
    {
        "key": "ctrl+alt+s",
        "command": "workbench.action.files.saveLocalFile",
        "when": "remoteFileDialogVisible"
    },
    {
        "key": "ctrl+shift+s",
        "command": "-workbench.action.files.saveLocalFile",
        "when": "remoteFileDialogVisible"
    },


    //! Extension: ReWrap
    //* Toggle ReWrap. Important to disable auto-wrap while in markdown files.
    {
        "key": "shift+alt+q",
        "command": "rewrap.toggleAutoWrap"
    },



    //! Extension: Shell Launcher
    {
        "key": "ctrl+alt+1",
        "command": "shellLauncher.launch"
    },
]
